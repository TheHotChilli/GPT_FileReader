{
    "manifest_version": 2,
    "name": "File Upload Extension",
    "version": "1.0",
    "description": "Extension to upload files",
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icon.png"
    },
    "permissions": [
      "activeTab",
      "storage",
      "<all_urls>"
    ]
  }
  